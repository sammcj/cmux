{
  "name": "@cmux/morphcloud-openapi-client",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "typecheck": "bunx tsgo --noEmit -p tsconfig.json",
    "lint": "eslint .",
    "generate": "bun run scripts/generate-openapi-client.ts"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./client": {
      "import": "./src/client/client/index.ts",
      "types": "./src/client/client/index.ts"
    },
    "./client.gen": {
      "import": "./src/client/client.gen.ts",
      "types": "./src/client/client.gen.ts"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@hey-api/client-fetch": "^0.13.1",
    "@hey-api/openapi-ts": "^0.82.1",
    "@types/node": "^24.9.1",
    "eslint": "^9.34.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.41.0"
  }
}
